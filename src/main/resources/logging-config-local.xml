<configuration>
<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
        <pattern>
            %blue(%d{HH:mm:ss.SSS}) %yellow([%thread]) %highlight(%-5level) %cyan(%logger{600}) - %msg%n
        </pattern>
    </encoder>
</appender>
<logger name="root" level="INFO"/>
<root level="INFO">
    <appender-ref ref="STDOUT" />
</root>
</configuration>

